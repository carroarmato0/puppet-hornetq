  <acceptors>
<% @acceptors.each do |acceptor| -%>
    <acceptor name="<%=  acceptor['name'] %>">
      <factory-class>org.hornetq.core.remoting.impl.netty.NettyAcceptorFactory</factory-class>
<% if  acceptor['params'] != nil -%>
<%  acceptor['params'].sort.each do |param_key, param_value| -%>
      <param key="<%= param_key %>" value="<%= param_value %>"/>
<% end -%>
<% end -%>
    </acceptor>
<% end -%>
  </acceptors>

